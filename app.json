{
  "expo": {
    "name": "bookmycuts",
    "slug": "bookmycuts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/scissors_icon.png",
    "scheme": "bookmycuts",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anazk.bookmycuts"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/scissors_icon.png",
        "backgroundColor": "#E10600"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anazk.bookmycuts",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
  "expo-router",
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#FF6B6B"
    }
  ],
  "expo-web-browser",
  [
    "expo-image-picker",
    {
      "photosPermission": "The app accesses your photos to let you share them with your friends."
    }
  ],
  [
  "expo-notifications",
  {
    "sounds": [
      "./assets/notification/scissorscuts.wav"
    ],
    "icon": "./assets/images/bookmycuts-new-wordmark.png",     
    "color": "#00aaff",
    "defaultChannel": "payment"                 
  }
]
],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6716cce4-1a91-4beb-af44-9acf810e8ff1"
      }
    }
  }
}
